{
  "name": "launchdarkly-flag-form",
  "version": "0.1.2",
  "description": "A minimal React hook-based library to control form field visibility and state using feature flags.",
  "main": "dist/flagged-form-lib.cjs.js",
  "module": "dist/flagged-form-lib.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/flagged-form-lib.es.js",
      "require": "./dist/flagged-form-lib.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "yup": "^1.2.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "yup": "^1.7.0",
    "zod": "^4.0.15"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "feature-flags",
    "launchdarkly",
    "schema",
    "zod",
    "yup",
    "conditional-rendering"
  ],
  "author": "Alec Aldrine Lakra",
  "license": "MIT"
}
